<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="GroupViewItem">
        <attr name="leftImage" format="integer"/>
        <attr name="title" format="string"/>
        <attr name="rightImage" format="integer"/>
        <attr name="itemHeight" format="dimension"/>
        <attr name="isShowLeft" format="boolean"/>
        <attr name="isShowRight" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="EcoGallery">
        <attr name="mygravity" format="integer" />
        <attr name="animationDuration" format="integer" />
        <attr name="unselectedAlpha" format="float" />
        <attr name="myspacing" format="dimension" />

        <!-- 选中的 Item 放大的倍数-->
        <attr name="selectedScale" format="float" />
    </declare-styleable>

    <attr name="ecoGalleryStyle" format="reference" />

    <declare-styleable name="CustomAbsSpinner">
        <attr name="entries" format="reference" />
    </declare-styleable>

    <attr name="customAbsSpinnerStyle" format="reference" />
</resources>